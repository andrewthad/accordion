cabal-version: 2.2
name: accordion-types
version: 0.1.0.0
build-type: Simple

library
  build-depends:
    , base >= 4.11.1 && < 5
    , transformers >= 0.5.6.2 && <0.6
    , nonempty-containers >= 0.1.1 && <0.2
    , containers >= 0.6.0.1 && <0.7
  hs-source-dirs: src
  default-language: Haskell2010
  ghc-options: -Wall
  exposed-modules:
    Accordion.Types
    Accordion.Nat
    Accordion.Fin.N5
    Accordion.Gte.N2
